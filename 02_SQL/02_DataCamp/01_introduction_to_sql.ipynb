    "\n",
    "# VIEW\n",
    "\n",
    "A view dont save data, only the query\n",
    "\n",
    "## create the view\n",
    "\n",
    "    CREATE VIEW employee_hire_years AS\n",
    "    SELECT id, name, year_hired\n",
    "    FROM employees;\n",
    "\n",
    "## use the views\n",
    "\n",
    "    SELECT id, name\n",
    "    FROM employee_hire_years;"
